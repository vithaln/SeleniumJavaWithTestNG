    <classes>
      <class name="com.selenium.code.TestNGDemo.crossBrowsers.CrossBrowseDemo"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Chrome">
  <parameter name="browserName" value="chrome"></parameter>
